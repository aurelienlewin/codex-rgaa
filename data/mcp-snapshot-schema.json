{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "doctype",
    "title",
    "lang",
    "dir",
    "href",
    "readyState",
    "images",
    "frames",
    "links",
    "formControls",
    "headings",
    "headingsSummary",
    "listItems",
    "langChanges",
    "dirChanges",
    "tables",
    "tableSummary",
    "fieldsets",
    "formSummary",
    "buttons",
    "landmarks",
    "focusables",
    "focusableSummary",
    "ariaLive",
    "ariaSummary",
    "rolesSummary",
    "meta",
    "linkSummary",
    "media",
    "mediaDetails",
    "visual",
    "scripts"
  ],
  "properties": {
    "doctype": { "type": "string" },
    "title": { "type": "string" },
    "lang": { "type": "string" },
    "dir": { "type": "string" },
    "href": { "type": "string" },
    "readyState": { "type": "string" },
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "role", "ariaHidden", "alt", "name"],
        "properties": {
          "tag": { "type": "string" },
          "role": { "type": "string" },
          "ariaHidden": { "type": "boolean" },
          "alt": { "type": ["string", "null"] },
          "name": { "type": "string" }
        }
      }
    },
    "frames": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["title", "ariaLabel", "ariaLabelledby"],
        "properties": {
          "title": { "type": "string" },
          "ariaLabel": { "type": "string" },
          "ariaLabelledby": { "type": "string" }
        }
      }
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "href",
          "name",
          "rawText",
          "title",
          "ariaLabel",
          "ariaLabelledby",
          "target",
          "rel"
        ],
        "properties": {
          "href": { "type": "string" },
          "name": { "type": "string" },
          "rawText": { "type": "string" },
          "title": { "type": "string" },
          "ariaLabel": { "type": "string" },
          "ariaLabelledby": { "type": "string" },
          "target": { "type": "string" },
          "rel": { "type": "string" }
        }
      }
    },
    "formControls": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "tag",
          "type",
          "id",
          "name",
          "label",
          "required",
          "ariaRequired",
          "autocomplete",
          "describedBy",
          "inFieldset",
          "fieldsetLegend"
        ],
        "properties": {
          "tag": { "type": "string" },
          "type": { "type": "string" },
          "id": { "type": "string" },
          "name": { "type": "string" },
          "label": { "type": "string" },
          "required": { "type": "boolean" },
          "ariaRequired": { "type": "boolean" },
          "autocomplete": { "type": "string" },
          "describedBy": { "type": "string" },
          "inFieldset": { "type": "boolean" },
          "fieldsetLegend": { "type": "string" }
        }
      }
    },
    "headings": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["level", "text"],
        "properties": {
          "level": { "type": "number" },
          "text": { "type": "string" }
        }
      }
    },
    "headingsSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": ["total", "h1", "h2", "h3", "h4", "h5", "h6"],
      "properties": {
        "total": { "type": "number" },
        "h1": { "type": "number" },
        "h2": { "type": "number" },
        "h3": { "type": "number" },
        "h4": { "type": "number" },
        "h5": { "type": "number" },
        "h6": { "type": "number" }
      }
    },
    "listItems": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["parent"],
        "properties": {
          "parent": { "type": "string" }
        }
      }
    },
    "langChanges": {
      "type": "array",
      "items": { "type": "string" }
    },
    "dirChanges": {
      "type": "array",
      "items": { "type": "string" }
    },
    "tables": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "hasTh",
          "hasCaption",
          "thCount",
          "thWithScope",
          "thWithId",
          "cellsWithHeaders",
          "hasThead",
          "hasTbody",
          "hasTfoot"
        ],
        "properties": {
          "hasTh": { "type": "boolean" },
          "hasCaption": { "type": "boolean" },
          "thCount": { "type": "number" },
          "thWithScope": { "type": "number" },
          "thWithId": { "type": "number" },
          "cellsWithHeaders": { "type": "number" },
          "hasThead": { "type": "boolean" },
          "hasTbody": { "type": "boolean" },
          "hasTfoot": { "type": "boolean" }
        }
      }
    },
    "tableSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "total",
        "withCaption",
        "withTh",
        "withScope",
        "withId",
        "withHeadersAttr",
        "withThead"
      ],
      "properties": {
        "total": { "type": "number" },
        "withCaption": { "type": "number" },
        "withTh": { "type": "number" },
        "withScope": { "type": "number" },
        "withId": { "type": "number" },
        "withHeadersAttr": { "type": "number" },
        "withThead": { "type": "number" }
      }
    },
    "fieldsets": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["legend", "hasLegend", "controlCount", "controls"],
        "properties": {
          "legend": { "type": "string" },
          "hasLegend": { "type": "boolean" },
          "controlCount": { "type": "number" },
          "controls": {
            "type": "array",
            "items": {
              "type": "object",
              "additionalProperties": false,
              "required": [
                "tag",
                "type",
                "id",
                "name",
                "label",
                "required",
                "ariaRequired",
                "autocomplete"
              ],
              "properties": {
                "tag": { "type": "string" },
                "type": { "type": "string" },
                "id": { "type": "string" },
                "name": { "type": "string" },
                "label": { "type": "string" },
                "required": { "type": "boolean" },
                "ariaRequired": { "type": "boolean" },
                "autocomplete": { "type": "string" }
              }
            }
          }
        }
      }
    },
    "formSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "controlsTotal",
        "missingLabel",
        "requiredCount",
        "autocompleteCount",
        "describedByCount",
        "inFieldsetCount",
        "fieldsetCount",
        "fieldsetWithLegendCount"
      ],
      "properties": {
        "controlsTotal": { "type": "number" },
        "missingLabel": { "type": "number" },
        "requiredCount": { "type": "number" },
        "autocompleteCount": { "type": "number" },
        "describedByCount": { "type": "number" },
        "inFieldsetCount": { "type": "number" },
        "fieldsetCount": { "type": "number" },
        "fieldsetWithLegendCount": { "type": "number" }
      }
    },
    "buttons": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "type", "role", "name"],
        "properties": {
          "tag": { "type": "string" },
          "type": { "type": "string" },
          "role": { "type": "string" },
          "name": { "type": "string" }
        }
      }
    },
    "landmarks": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "role", "label"],
        "properties": {
          "tag": { "type": "string" },
          "role": { "type": "string" },
          "label": { "type": "string" }
        }
      }
    },
    "focusables": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "role", "tabindex", "type"],
        "properties": {
          "tag": { "type": "string" },
          "role": { "type": "string" },
          "tabindex": { "type": "number" },
          "type": { "type": "string" }
        }
      }
    },
    "focusableSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": ["total", "tabindexPositive", "tabindexZero", "maxTabindex"],
      "properties": {
        "total": { "type": "number" },
        "tabindexPositive": { "type": "number" },
        "tabindexZero": { "type": "number" },
        "maxTabindex": { "type": "number" }
      }
    },
    "ariaLive": {
      "type": "object",
      "additionalProperties": false,
      "required": ["liveRegions", "rolesCount", "politeness", "roles"],
      "properties": {
        "liveRegions": { "type": "number" },
        "rolesCount": { "type": "number" },
        "politeness": {
          "type": "object",
          "additionalProperties": false,
          "required": ["polite", "assertive", "off"],
          "properties": {
            "polite": { "type": "number" },
            "assertive": { "type": "number" },
            "off": { "type": "number" }
          }
        },
        "roles": {
          "type": "object",
          "additionalProperties": false,
          "required": ["alert", "status", "log", "marquee", "timer"],
          "properties": {
            "alert": { "type": "number" },
            "status": { "type": "number" },
            "log": { "type": "number" },
            "marquee": { "type": "number" },
            "timer": { "type": "number" }
          }
        }
      }
    },
    "ariaSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": ["label", "labelledby", "describedby", "hidden"],
      "properties": {
        "label": { "type": "number" },
        "labelledby": { "type": "number" },
        "describedby": { "type": "number" },
        "hidden": { "type": "number" }
      }
    },
    "rolesSummary": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["role", "count"],
        "properties": {
          "role": { "type": "string" },
          "count": { "type": "number" }
        }
      }
    },
    "meta": {
      "type": "object",
      "additionalProperties": false,
      "required": ["viewport", "refresh"],
      "properties": {
        "viewport": { "type": "string" },
        "refresh": { "type": "string" }
      }
    },
    "linkSummary": {
      "type": "object",
      "additionalProperties": false,
      "required": ["total", "targetBlank", "targetBlankNoRel", "fragmentLinks"],
      "properties": {
        "total": { "type": "number" },
        "targetBlank": { "type": "number" },
        "targetBlankNoRel": { "type": "number" },
        "fragmentLinks": { "type": "number" }
      }
    },
    "media": {
      "type": "object",
      "additionalProperties": false,
      "required": ["video", "audio", "object"],
      "properties": {
        "video": { "type": "number" },
        "audio": { "type": "number" },
        "object": { "type": "number" }
      }
    },
    "mediaDetails": {
      "type": "object",
      "additionalProperties": false,
      "required": ["videos", "audios"],
      "properties": {
        "videos": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "hasControls",
              "autoplay",
              "muted",
              "loop",
              "hasCaptions",
              "hasSubtitles",
              "hasDescriptions",
              "hasChapters",
              "hasMetadata"
            ],
            "properties": {
              "hasControls": { "type": "boolean" },
              "autoplay": { "type": "boolean" },
              "muted": { "type": "boolean" },
              "loop": { "type": "boolean" },
              "hasCaptions": { "type": "boolean" },
              "hasSubtitles": { "type": "boolean" },
              "hasDescriptions": { "type": "boolean" },
              "hasChapters": { "type": "boolean" },
              "hasMetadata": { "type": "boolean" }
            }
          }
        },
        "audios": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "hasControls",
              "autoplay",
              "muted",
              "loop",
              "hasCaptions",
              "hasSubtitles",
              "hasDescriptions",
              "hasChapters",
              "hasMetadata"
            ],
            "properties": {
              "hasControls": { "type": "boolean" },
              "autoplay": { "type": "boolean" },
              "muted": { "type": "boolean" },
              "loop": { "type": "boolean" },
              "hasCaptions": { "type": "boolean" },
              "hasSubtitles": { "type": "boolean" },
              "hasDescriptions": { "type": "boolean" },
              "hasChapters": { "type": "boolean" },
              "hasMetadata": { "type": "boolean" }
            }
          }
        }
      }
    },
    "visual": {
      "type": "object",
      "additionalProperties": false,
      "required": ["svg", "canvas", "picture", "cssBackgroundImages", "bgExamples"],
      "properties": {
        "svg": { "type": "number" },
        "canvas": { "type": "number" },
        "picture": { "type": "number" },
        "cssBackgroundImages": { "type": "number" },
        "bgExamples": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["tag", "id", "className", "ariaHidden", "backgroundImage"],
            "properties": {
              "tag": { "type": "string" },
              "id": { "type": "string" },
              "className": { "type": "string" },
              "ariaHidden": { "type": "boolean" },
              "backgroundImage": { "type": "string" }
            }
          }
        }
      }
    },
    "scripts": {
      "type": "object",
      "additionalProperties": false,
      "required": ["scriptTags", "hasInlineHandlers"],
      "properties": {
        "scriptTags": { "type": "number" },
        "hasInlineHandlers": { "type": "boolean" }
      }
    }
  }
}
