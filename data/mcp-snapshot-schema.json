{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "doctype",
    "title",
    "lang",
    "href",
    "readyState",
    "images",
    "frames",
    "links",
    "formControls",
    "headings",
    "listItems",
    "langChanges",
    "tables",
    "media",
    "visual",
    "scripts"
  ],
  "properties": {
    "doctype": { "type": "string" },
    "title": { "type": "string" },
    "lang": { "type": "string" },
    "href": { "type": "string" },
    "readyState": { "type": "string" },
    "images": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "role", "ariaHidden", "alt", "name"],
        "properties": {
          "tag": { "type": "string" },
          "role": { "type": "string" },
          "ariaHidden": { "type": "boolean" },
          "alt": { "type": ["string", "null"] },
          "name": { "type": "string" }
        }
      }
    },
    "frames": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["title", "ariaLabel", "ariaLabelledby"],
        "properties": {
          "title": { "type": "string" },
          "ariaLabel": { "type": "string" },
          "ariaLabelledby": { "type": "string" }
        }
      }
    },
    "links": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["href", "name", "rawText", "title", "ariaLabel", "ariaLabelledby"],
        "properties": {
          "href": { "type": "string" },
          "name": { "type": "string" },
          "rawText": { "type": "string" },
          "title": { "type": "string" },
          "ariaLabel": { "type": "string" },
          "ariaLabelledby": { "type": "string" }
        }
      }
    },
    "formControls": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["tag", "type", "id", "name", "label"],
        "properties": {
          "tag": { "type": "string" },
          "type": { "type": "string" },
          "id": { "type": "string" },
          "name": { "type": "string" },
          "label": { "type": "string" }
        }
      }
    },
    "headings": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["level", "text"],
        "properties": {
          "level": { "type": "number" },
          "text": { "type": "string" }
        }
      }
    },
    "listItems": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["parent"],
        "properties": {
          "parent": { "type": "string" }
        }
      }
    },
    "langChanges": {
      "type": "array",
      "items": { "type": "string" }
    },
    "tables": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["hasTh", "hasCaption"],
        "properties": {
          "hasTh": { "type": "boolean" },
          "hasCaption": { "type": "boolean" }
        }
      }
    },
    "media": {
      "type": "object",
      "additionalProperties": false,
      "required": ["video", "audio", "object"],
      "properties": {
        "video": { "type": "number" },
        "audio": { "type": "number" },
        "object": { "type": "number" }
      }
    },
    "visual": {
      "type": "object",
      "additionalProperties": false,
      "required": ["svg", "canvas", "picture", "cssBackgroundImages", "bgExamples"],
      "properties": {
        "svg": { "type": "number" },
        "canvas": { "type": "number" },
        "picture": { "type": "number" },
        "cssBackgroundImages": { "type": "number" },
        "bgExamples": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": false,
            "required": ["tag", "id", "className", "ariaHidden", "backgroundImage"],
            "properties": {
              "tag": { "type": "string" },
              "id": { "type": "string" },
              "className": { "type": "string" },
              "ariaHidden": { "type": "boolean" },
              "backgroundImage": { "type": "string" }
            }
          }
        }
      }
    },
    "scripts": {
      "type": "object",
      "additionalProperties": false,
      "required": ["scriptTags", "hasInlineHandlers"],
      "properties": {
        "scriptTags": { "type": "number" },
        "hasInlineHandlers": { "type": "boolean" }
      }
    }
  }
}
