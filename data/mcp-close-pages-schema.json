{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "required": ["closed", "skipped", "errors"],
  "properties": {
    "closed": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "url", "title"],
        "properties": {
          "id": { "type": "number" },
          "url": { "type": "string" },
          "title": { "type": "string" }
        }
      }
    },
    "skipped": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "url", "reason"],
        "properties": {
          "id": { "type": "number" },
          "url": { "type": "string" },
          "reason": { "type": "string" }
        }
      }
    },
    "errors": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["id", "url", "error"],
        "properties": {
          "id": { "type": "number" },
          "url": { "type": "string" },
          "error": { "type": "string" }
        }
      }
    }
  }
}
