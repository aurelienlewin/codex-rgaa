{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "pages": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "number" },
          "url": { "type": "string" },
          "title": { "type": ["string", "null"] }
        },
        "required": ["id", "url", "title"],
        "additionalProperties": false
      }
    }
  },
  "required": ["pages"]
}
